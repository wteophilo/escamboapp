<h1 class="panel-heading">Administradores</h1>
<%if @admin.errors.any?%>
    <div class="alert alert-danger">
        <ul>
            <%= @admin.errors.full_messages.each do |message|%>
            <li><%=message%></li>
            <%end%>
        </ul>
         
    </div>
<%end%>

<!--begin panel-->
<div class = "panel panel-default">
    <div class="panel-heading">
        <%=action%>
    </div>
    <div class ="panel-body">
        <div class="col-lg-6">
            <div class ="form-group">
                <label>Descrição</label>
                 <%= form_for([:backoffice,@admin]) do |f|%>
                    <%= f.text_field :name, class: "form-control" ,placeholder: "Nome do Administrador" %>
                    <%= f.text_field :email, class: "form-control" ,placeholder: "Email do administrador" %>
                    <%= f.password_field :password, class: "form-control" ,placeholder: "Ex: 123456" %>
                    <%= f.password_field :password_confirmation, class: "form-control" ,placeholder: "Ex: 123456" %>
                    <button type="submit" class="btn btn-default">Salvar</button>
                <%end%>
              </div>
        </div>
    </div>
</div>
<!-- End Panel
          